{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "app:default",
    "event:default",
    "menu:default",
    "os:allow-locale",
    "path:default",
    "resources:default",
    "store:allow-get",
    "store:allow-save",
    "store:allow-set",
    "tray:default",
    "window:default",
    "webview:default"
  ],
  "platforms": ["macOS", "windows"]
}
